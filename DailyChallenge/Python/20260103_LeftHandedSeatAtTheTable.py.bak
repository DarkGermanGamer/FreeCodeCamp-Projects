def find_left_handed_seats(table):
    righthanded = "R"
    unoccupied = "U"
    
    possibleSeatCount = 0

    for rowCount, row in enumerate(table):        
        for seatCount, seat in enumerate(row):            
            leftSeat = unoccupied

            if rowCount % 2:
                if seatCount - 1 >= 0:
                    leftSeat = row[seatCount - 1]
            else:
                if seatCount + 1 < len(row):
                    leftSeat = row[seatCount + 1]

            if seat == unoccupied and leftSeat != righthanded:
                possibleSeatCount += 1

    return possibleSeatCount;